<body >
    <div class="container ">
<div class="row  ">
 
<h2>ORDERS</h2>
    <div class="col-md-12">
 <table class="table mt-3">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Date</th>
      <th scope="col">Address</th>
      <th scope="col">Place</th>
      <th scope="col">Mobile</th>
      <th scope="col">â‚¹Amount</th>
      <th scope="col">Payment</th>
      <th scope="col">Status</th>
      <th scope="col">Options</th>
    </tr>
  </thead>
  <tbody>
    {{#each orders}}
    <tr>
      <th scope="row">{{this.deliveryDetails.date}}</th>
      <td>{{this.deliveryDetails.address}}</td>
      <td>{{this.deliveryDetails.place}}</td>
      <td>{{this.deliveryDetails.mobile}}</td>
      <td>{{this.deliveryDetails.total}}</td>
      <td>{{this.paymentMethode}}</td>
      <td>{{this.status}}</td>
     
       <td>
        {{!-- <a href="/admin/editproduct/{{this._id}}" class="btn btn-info font-weight-bold">Edit</a> --}}
        <a href="/view-order-product/{{this._id}}" class="btn btn-success font-weight-bold" >View</a>
        {{#if this.status!='delivered'}}
        <a href="/cancelorder/{{this._id}}" class="btn btn-danger " onclick="return confirm('DO you want to cancel the order?')">cancel</a>
       {{/if}}
        </td>
    
    </tr>
    {{/each}}
    
  </tbody>
</table>
</div>
</div>
</div>
  

